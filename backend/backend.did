type issue = record {
    state: opt text;
    login: opt text;
    id: opt text;
    milestone_state: opt text;
    closed_at: opt text;
};

type gh_token = text;

service : {
    "get_gh_issue": (gh_token) -> (issue);
    "get_gh_fixed_by": (gh_token) -> (text);
    "get_gh_is_merged": (gh_token) -> (text);   
}

