type Issue = record {
    state: opt text;
    login: opt text;
    id: opt text;
    milestone_state: opt text;
    closed_at: opt text;
};

type GithubToken = text;

service : {
    "get_gh_issue": (GithubToken) -> (Issue);
    "get_gh_fixed_by": (GithubToken) -> (text);
    "get_gh_is_merged": (GithubToken) -> (text);   
}

